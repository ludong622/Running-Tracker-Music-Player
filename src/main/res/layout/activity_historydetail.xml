<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#d5ccf0"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map3"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/result_date"
        android:layout_marginBottom="20dp"
        tools:context="comp5216.running.ResultActivity" />

    <TextView
        android:id="@+id/result_date"
        android:layout_width="227dp"
        android:layout_height="20dp"
        android:layout_above="@+id/result_time"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:id="@+id/result_time"
        android:layout_width="227dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/result_distance"
        android:layout_marginLeft="10dp"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginBottom="10dp"/>


    <TextView
        android:id="@+id/result_distance"
        android:layout_width="227dp"
        android:layout_height="20dp"
        android:layout_above="@+id/result_speed"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:id="@+id/result_speed"
        android:layout_width="227dp"
        android:layout_height="20dp"
        android:layout_above="@+id/result_pace"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:id="@+id/result_pace"
        android:layout_width="227dp"
        android:layout_height="20dp"
        android:layout_above="@+id/stopbutton"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginBottom="20dp"/>

    <Button
        android:id="@+id/stopbutton"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/shape3"
        android:onClick="Back"
        android:text="Back"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteY="396dp" />

</RelativeLayout>